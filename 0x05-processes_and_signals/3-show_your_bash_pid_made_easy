#!/bin/env bash

# Find the PID and process name of processes containing the word "bash"
for pid in /proc/*/cmdline; do
    if grep -q "bash" "$pid"; then
        pid_number=$(echo "$pid" | cut -d '/' -f 3)
        process_name=$(ps -p "$pid_number" -o comm=)
        echo "$pid_number $process_name"
    fi
done

